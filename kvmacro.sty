%  Soundness
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand{\tripleSem}[5]{%
#1 \vDash_{#2} %
    \begin{array}{@{}l@{}} %
    \triple{#3}{#4}{#5} %
    \end{array}%
}
\newcommand{\tripleSemG}[3]{%
    \tripleSem{}{g}{#1}{#2}{#3}
}
